{
    "name": "mathiasverraes/uptodocs",
    "description": "UpToDocs scans a Markdown file for PHP code blocks, and executes each one in a separate process. Include this in your CI workflows, to make sure your documentation is always up to date with your code.",
    "type": "library",
    "keywords": ["documentation","testing", "CI"],
    "homepage": "https://github.com/mathiasverraes/uptodocs",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "league/commonmark": "^1.4",
        "symfony/process": "^5.0",
        "symfony/console": "^5.0"
    },
    "authors": [
        {
            "name": "Mathias Verraes",
            "email": "mathias@verraes.net",
            "homepage": "https://verraes.net",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Verraes\\UpToDocs\\": "src/"
        }
    },
    "bin": ["uptodocs"]
}
